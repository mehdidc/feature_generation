{
    "model_name": "model101",
    "model_params": {
        "nb_filters": [8, 16, 128, 128], 
        "filter_size": 3,
        "weight_sharing": true,
        "sparse_func": "max_k_spatial",
        "k": [16, 8, 4, 2],
        "merge_op": "sum"
    },
    "dataset": "loader",
    "data_params":{
        "pipeline": [
                {"name": "imagefilelist", "params": {"pattern": "{gametiles}"}},
                {"name": "shuffle", "params": {}},
                {"name": "imageread", "params": {}},
                {"name": "normalize_shape", "params": {}},
                {"name": "force_rgb", "params": {}},
                {"name": "resize", "params": {"shape": [16, 16]}},
                {"name": "divide_by", "params": {"value": 255}},
                {"name": "order", "params": {"order": "th"}}
            ]
    },
    "mode": "random",
    "budget_hours": 24,
    "optimization": {
        "algo": "adadelta",
        "initial_lr": 0.1
    },
    "report_rec_error": false
}
