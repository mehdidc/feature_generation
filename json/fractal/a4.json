{
"comment": "brush / rescaled/ with black borders / denoising / 16x16",
"autoencoding_loss": "squared_error",
"binarize_thresh": null,
"budget_hours": 10,
"contractive": false,
"contractive_coef": null,
"dataset": "rescaled_digits",
"denoise": 0.5,
"noise": "salt_and_pepper", 
"marginalized": false,
"model_name": "model83",
"model_params": {   
    "n_steps": 30,
    "nb_fc_layers": 1,
    "nb_fc_units": [100],
    "nb_recurrent_layers": 1,
    "nb_recurrent_units": 100,
    "nonlin": "very_leaky_rectify",
    "patch_size": 2,
    "reduce_func": "sum",
    "normalize_func": "sigmoid",
    "x_sigma": 0.5,
    "y_sigma": 0.5,
    "x_stride": 1,
    "y_stride": 1,
    "color": "predicted",
    "x_min": 0,
    "x_max": "width",
    "y_min": 0,
    "y_max": "height",
    "recurrent_model": "rnn"
},
"optimization": {
    "max_nb_epochs": 9999999999,
    "patience_nb_epochs": 500
},
"force_w": 16,
"force_h": 16,
"walkback": 1,
"mode": "random",
"report_rec_error": false,
"walkback_mode": "bengio_without_sampling"
}
