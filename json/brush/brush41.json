{
"budget_hours": 10,
"dataset": "loader",
"data_params":{
    "pipeline": [
            {"name": "imagefilelist", "params": {"pattern": "{shoes}"}},
            {"name": "limit", "params": {"nb": 100}},
            {"name": "repeat", "params": {"nb": 10}},
            {"name": "shuffle", "params": {}},
            {"name": "imageread", "params": {}},
            {"name": "normalize_shape", "params": {}},
            {"name": "crop", "params": {"shape": [64, 64], "pos": "center", "mode": "constant"}},
            {"name": "crop", "params": {"shape": [16, 16], "pos": "random_inside", "mode": "reflect"}},
            {"name": "divide_by", "params": {"value": 255}},
            {"name": "order", "params": {"order": "th"}}
        ]
},
"model_name": "model97",
"model_params": {   
        "n_steps": 20,
        "nb_fc_layers": 1,
        "nb_fc_units": [32],
        "nb_recurrent_units": 256,
        "nonlin": "very_leaky_rectify",
        "patch_size": 8,
        "reduce_func": "sum",
        "nonlin_out": "sigmoid",
        "normalize_func": "sigmoid",
        "proba_func": "softmax",
        "x_sigma": 1,
        "y_sigma": 1,
        "x_stride": [0.125, 0.25, 0.5, 1],
        "y_stride": [0.125, 0.25, 0.5, 1],
        "color": "predicted",
        "x_min": 0,
        "x_max": "width",
        "y_min": 0,
        "y_max": "height",
        "nb_patches": 1,
        "learn_patches": false,
        "patch_index": 0
},
"optimization": {
    "algo": "adadelta",
    "learning_rate": 0.01,
    "max_nb_epochs": 9999999999,
    "patience_nb_epochs": 500
},
"report_rec_error": false
}
