{
"budget_hours": 10,
"dataset": "loader",
"data_params":{
    "pipeline": [
        {"name": "dataset", "params": {"name": "mnist", "which":"train"}},
        {"name": "limit", "params":{"nb": 10}},
        {"name": "order", "params": {"order": "tf"}},
        {"name": "shuffle", "params": {}},
        {"name": "normalize_shape", "params": {}},
        {"name": "random_colorize", "params":{"op": "threshold_inv"}},
        {"name": "resize", "params": {"shape": [16, 16]}},
        {"name": "force_rgb", "params": {}},
        {"name": "divide_by", "params": {"value": 255}},
        {"name": "order", "params": {"order": "th"}}
    ]
},
"model_name": "model88",
"model_params": {   
        "n_steps": 20,
        "nb_fc_layers": 1,
        "nb_fc_units": [32],
        "nb_recurrent_units": [64],
        "nonlin": "very_leaky_rectify",
        "reduce_func": "sum",
        "color_min": -1,
        "color_max": 1,
        "nonlin_out": "sigmoid",
        "recurrent_model": "gru",
        "normalize_func": "sigmoid",
        "proba_func": "softmax",
        "x_sigma": 1,
        "y_sigma": 1,
        "x_stride": "predicted",
        "y_stride": "predicted",
        "stride_normalize": true,
        "patch_size": 16,
        "color": "predicted",
        "x_min": 0,
        "x_max": "width",
        "y_min": 0,
        "y_max": "height",
        "parallel": 1,
        "nb_patches": 1,
        "patch_index": 0,
        "learn_patches": false,
        "w_left_pad": "half_patch",
        "w_right_pad": "half_patch",
        "h_left_pad": "half_patch",
        "h_right_pad": "half_patch"
},
"optimization": {
    "algo": "adadelta",
    "learning_rate": 0.01,
    "max_nb_epochs": 9999999999,
    "patience_nb_epochs": 500
},
"report_rec_error": false
}
