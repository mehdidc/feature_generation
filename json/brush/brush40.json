{
"budget_hours": 10,
"dataset": "loader",
"data_params":{
    "pipeline": [
            {"name": "imagefilelist", "params": {"pattern": "{shoes}"}},
            {"name": "limit", "params": {"nb": 100}},
            {"name": "repeat", "params": {"nb": 10}},
            {"name": "shuffle", "params": {}},
            {"name": "imageread", "params": {}},
            {"name": "normalize_shape", "params": {}},
            {"name": "crop", "params": {"shape": [64, 64], "pos": "center", "mode": "constant"}},
            {"name": "crop", "params": {"shape": [16, 16], "pos": "random_inside", "mode": "reflect"}},
            {"name": "divide_by", "params": {"value": 255}},
            {"name": "order", "params": {"order": "th"}}
        ]
},
"model_name": "model88",
"model_params": {   
        "n_steps": 80,
        "nb_fc_layers": 1,
        "nb_fc_units": [300],
        "nb_recurrent_layers": 2,
        "nb_recurrent_units": [32, 64],
        "nonlin": "very_leaky_rectify",
        "patch_size": 2,
        "reduce_func": "over",
        "nonlin_out": "msigmoid",
        "normalize_func": "sigmoid",
        "x_sigma": 0.5,
        "y_sigma": 0.5,
        "x_stride": 1,
        "y_stride": 1,
        "color": "predicted",
        "x_min": 0,
        "x_max": "width",
        "y_min": 0,
        "y_max": "height",
        "recurrent_model": "lstm",
        "parallel": 2,
        "parallel_reduce_func": "sum",
        "parallel_share": false
},
"optimization": {
    "algo": "adadelta",
    "max_nb_epochs": 9999999999,
    "patience_nb_epochs": 500
},
"report_rec_error": false
}
