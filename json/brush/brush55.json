{
"budget_hours": 10,
"dataset": "loader",
"data_params":{
    "pipeline": [
            {"name": "imagefilelist", "params": {"pattern": "gametiles/zw-tilesets/img/*.png"}},
            {"name": "shuffle", "params": {}},
            {"name": "imageread", "params": {}},
            {"name": "normalize_shape", "params": {}},
            {"name": "force_rgb", "params": {}},
            {"name": "resize", "params": {"shape": [16, 16]}},
            {"name": "divide_by", "params": {"value": 255}},
            {"name": "order", "params": {"order": "th"}}
        ]
},
"model_name": "model99",
"model_params": {   
        "n_steps": 30,
        "nb_fc_layers": 1,
        "nb_fc_units": [32],
        "nb_recurrent_units": 256,
        "nonlin": "very_leaky_rectify",
        "patch_size": 4,
        "reduce_func": "over",
        "nonlin_out": "msigmoid",
        "normalize_func": "sigmoid",
        "proba_func": "softmax",
        "x_sigma": 1,
        "y_sigma": 1,
        "x_stride": 1,
        "y_stride": 1,
        "color": "patches",
        "x_min": 0,
        "x_max": "width",
        "y_min": 0,
        "y_max": "height",
        "nb_patches": 128,
        "learn_patches": true,
        "patch_index": "predicted"
},
"optimization": {
    "algo": "adadelta",
    "learning_rate": 0.01,
    "max_nb_epochs": 9999999999,
    "patience_nb_epochs": 500
},
"report_rec_error": false
}
