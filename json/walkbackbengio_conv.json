{
    "binarize_thresh": 0.5,
    "noise": "salt_and_pepper",
    "contractive_coef": null,
    "denoise": 0.3,
    "walkback": 1,
    "marginalized": false,
    "autoencoding_loss": "cross_entropy",
    "walkback_mode": "bengio_without_sampling",
    "model_name": "model55",

    "dataset": "flaticon",
    "budget_hours": 8,
    "mode": "minibatch",
    "optimization": {
        "algo": "adadelta",
        "initial_lr": 0.001
    },
    "model_params":{
      "nb_filters": 128,
      "nb_filters_mul": 1,
      "nb_layers": 4,
      "use_wta_channel": false,
      "use_wta_spatial": false
    }
}
